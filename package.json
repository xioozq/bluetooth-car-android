{
  "name": "bluetooth-car-android",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "reset": "npm run start --reset-cache",
    "start-ad:staging": "react-native run-android --variant releaseStaging",
    "start-ad:release": "react-native run-android --variant release",
    "build-ad:staging": "cd android/ && ./gradlew assembleReleaseStaging && cd ../",
    "build-ad:release": "cd android/ && ./gradlew assembleRelease && cd ../"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.0",
    "dva": "^2.4.1",
    "dva-loading": "^3.0.6",
    "lottie-react-native": "^2.6.1",
    "md5": "^2.2.1",
    "react": "16.8.3",
    "react-native": "0.62.3",
    "react-native-bluetooth-serial": "^1.0.0-rc1",
    "react-native-elements": "^1.2.7",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-largelist-v3": "^3.0.14",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-loader": "^1.2.1",
    "react-native-parallax-scroll-view": "git+https://github.com/xioozq/react-native-parallax-scroll-view",
    "react-native-permissions": "^1.1.1",
    "react-native-scrollable-tab-view": "^0.10.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-spring-scrollview": "^2.0.22",
    "react-native-svg": "^9.5.1",
    "react-navigation": "^3.11.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-root-import": "^6.2.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
